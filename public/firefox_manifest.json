{
  "name": "NeverMarket",
  "version": "1.0",
  "description": "Remove Everyday Market items from Woolworths search pages.",
  "permissions": [
    "*://www.woolworths.com.au/*"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.woolworths.com.au/shop/search/*"],
      "js": ["content_scripts/woolworths.js"]
    }
  ],
  "icons": {
    "16": "images/logo/date-time-16.png",
    "32": "images/logo/date-time-32.png",
    "48": "images/logo/date-time-48.png",
    "128": "images/logo/date-time-128.png"
  }
}